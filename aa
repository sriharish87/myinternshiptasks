

ps aux | grep httpd
csudo chcon -R -t httpd_sys_rw_content_t /var/www/drupal/sites/default/files
sudo chcon -t httpd_sys_rw_content_t /var/www/drupal/sites/default/settings.php
sudo chown -R apache:apache /var/www/drupal
sudo find /var/www/drupal -type d -exec chmod 755 {} \;
sudo find /var/www/drupal -type f -exec chmod 644 {} \;
sudo chmod 775 /var/www/drupal/sites/default/files
sudo chmod 644 /var/www/drupal/sites/default/settings.php
